<?xml-stylesheet href="html.xslt" type="text/xsl"?>
	
<man name="startObject" long-name="Execute the code attached to an abject verb">
	<description>	
	
	
	Execute the code attached to an abject verb. Flags are the same as for
	scripts: bit 0 freeze resistant, bit 1 recursive.
	
	
	</description>
	<command name="startObject">
		<file name="flags" required="true"/>
		<file name="obj" required="true"/>
		<file name="verb" required="true"/>
		<file name="[args]" required="true"/>
	</command>
	<see-also>
<cmd>startScript</cmd>, <cmd>startScript0</cmd>, <cmd>startScript2</cmd>, <cmd>startRoom</cmd>, <cmd>startRoomWithEgo</cmd>, <cmd>startObject2</cmd>, <cmd>startSound</cmd>, <cmd>startMusic</cmd>	</see-also>
</man>