<?xml-stylesheet href="html.xslt" type="text/xsl"?>
	
<man name="stampObject" long-name="Dunno really what this function is about">
	<description>	
	
	
	Dunno really what this function is about. It basically just set the object
	state, optionaly setting the object position to x\*8,y\*8 if x != -1.
	
	
	</description>
	<command name="stampObject">
		<file name="obj" required="true"/>
		<file name="x" required="true"/>
		<file name="y" required="true"/>
		<file name="state" required="true"/>
	</command>
</man>