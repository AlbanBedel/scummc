<man name="walkActorTo" long-name="Make an actor walk to the given position">
	<description>	
	
	
	Make an actor walk to the given position.
	
	
	</description>
	<command name="walkActorTo">
		<file name="actor" required="true"/>
		<file name="x" required="true"/>
		<file name="y" required="true"/>
	</command>
	<see-also>
<cmd>walkActorToObj</cmd>	</see-also>
</man>