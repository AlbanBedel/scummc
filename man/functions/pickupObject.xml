<?xml-stylesheet href="html.xslt" type="text/xsl"?>
	
<man name="pickupObject" long-name="Make VAR\_EGO the owner of the object, set the object as untouchable, turn">
	<description>	
	
	
	Make VAR\_EGO the owner of the object, set the object as untouchable, turn
	it into state 1 and run the inventory script. It's generaly just what is
	needed to pick an object from the room.
	
	
	</description>
	<command name="pickupObject">
		<file name="obj" required="true"/>
		<file name="room" required="true"/>
	</command>
	<see-also>
<cmd>pickOneOf</cmd>, <cmd>pickOneOfDefault</cmd>	</see-also>
</man>