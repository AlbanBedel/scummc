<?xml-stylesheet href="html.xslt" type="text/xsl"?>
	
<man name="startRoom" long-name="Start the given room">
	<description>	
	
	
	Start the given room.
	
	WARNING: In scummvm this function is named loadRoom. However it is clashing
	with the ressource functions. So I renamed this one startRoom which anyway
	better reflect what it do.
	
	
	</description>
	<command name="startRoom">
		<file name="room" required="true"/>
	</command>
	<see-also>
<cmd>startScript</cmd>, <cmd>startScript0</cmd>, <cmd>startScript2</cmd>, <cmd>startRoomWithEgo</cmd>, <cmd>startObject</cmd>, <cmd>startObject2</cmd>, <cmd>startSound</cmd>, <cmd>startMusic</cmd>	</see-also>
</man>